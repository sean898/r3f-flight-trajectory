{
    "name": "flight_path",
    "version": "0.0.3",
    "description": "3D trajectory plot of aircraft flight data",
    "main": "build/index.js",
    "scripts": {
        "start": "webpack-serve --config ./webpack.serve.config.js",
        "validate-init": "python _validate_init.py",
        "prepublishOnly": "npm run validate-init",
        "build:js": "webpack --mode production",
        "build:backends": "dash-generate-components ./src/lib/components flight_path -p package-info.json --r-prefix '' --jl-prefix '' --ignore \\.test\\.",
        "build:backends-activated": "(. venv/bin/activate || venv\\scripts\\activate && npm run build:py_and_r)",
        "build": "npm run build:js && npm run build:backends",
        "build:activated": "npm run build:js && npm run build:backends-activated",
        "py": "npm run build && python usage.py"
    },
    "author": "Sean Reedy <15895395+sean898@users.noreply.github.com>",
    "license": "",
    "dependencies": {
        "@react-spring/three": "^9.5.4",
        "@react-three/drei": "8.20.2",
        "@react-three/fiber": "7.0.29",
        "file-loader": "^6.2.0",
        "ramda": "^0.28.0",
        "react": "^17.0.2",
        "three": "^0.138.3"
    },
    "devDependencies": {
        "@babel/core": "^7.18.10",
        "@babel/plugin-proposal-object-rest-spread": "^7.18.9",
        "@babel/preset-env": "^7.18.10",
        "@babel/preset-react": "^7.18.6",
        "@plotly/dash-component-plugins": "^1.2.3",
        "@plotly/webpack-dash-dynamic-import": "^1.3.0",
        "babel-eslint": "^10.0.2",
        "babel-loader": "^8.2.5",
        "copyfiles": "^2.1.1",
        "css-loader": "^3.0.0",
        "eslint": "^8.22.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-react": "^7.30.1",
        "papaparse": "^5.3.2",
        "prop-types": "^15.8.1",
        "react-docgen": "^5.4.3",
        "react-dom": "^17.0.2",
        "style-loader": "^0.23.1",
        "styled-jsx": "^5.0.4",
        "terser-webpack-plugin": "^2.3.0",
        "webpack": "4.36.1",
        "webpack-cli": "3.3.6",
        "webpack-serve": "3.1.0"
    },
    "engines": {
        "node": ">=8.11.0",
        "npm": ">=6.1.0"
    }
}
